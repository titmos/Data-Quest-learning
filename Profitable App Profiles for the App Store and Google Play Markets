    "duplicates = []\n",
    "unique = []\n",
    "for app in android[1:]:\n",
    "    name = app[0]\n",
    "    if name in unique:\n",
    "        duplicates.append(name)\n",
    "    else:\n",
    "        unique.append(name)\n",
    "print('Number of duplicates found : ', len(duplicates))\n",
    "print('List of duplicated apps ', duplicates[:5], '\\n')\n",
    "print(android[:10])"
   ]
